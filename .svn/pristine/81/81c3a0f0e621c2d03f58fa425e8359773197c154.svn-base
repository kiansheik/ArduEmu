package poolsync;

public class PObj extends PoolObject {
	private boolean out = false;
	@Override
	public void init() {}

	@Override
	public void step() {
		if(out == false) this.poolOutput.accept("Testing");
		out = true;
	}

	@Override
	public void reset() {
		out = false;
	}

	@Override
	public boolean isDone() {
		return out = true;
	}
}

