package emulator.command;

import emulator.memory.MemData;
import emulator.memory.MemEEProm;
import emulator.memory.MemProg;

public abstract class Command {

    protected static int getRrAddrCat1(short inst) {
        return ((inst & 0x200) >>> 5) | 0xf & inst;
    }
    protected static int getRdAddrCat1(short inst) {
        return (inst >>> 4) & 0x1f;
    }
    public abstract void run(MemData memData, MemProg memProg, MemEEProm memEEProm, short instr);
}
